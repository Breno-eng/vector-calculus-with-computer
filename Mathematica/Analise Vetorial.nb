    OverscriptBox["i", "^"]}], "+", 
   RowBox[{"Q", 
    RowBox[{"(", 
     RowBox[{"x", ",", "y", ",", "z"}], ")"}], 
    OverscriptBox["j", "^"]}], "+", 
