Cell[BoxData[
 FormBox[
  RowBox[{"\t\t\t\t", 
   RowBox[{
    RowBox[{
     OverscriptBox["r", "\[RightVector]"], "(", 
     RowBox[{"\[Phi]", ",", "\[Theta]"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"r", " ", "Cos\[Phi]"}], "+", "R"}], ")"}], " ", 
      "Cos\[Theta]", " ", 
      OverscriptBox["i", "^"]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"r", " ", "Cos\[Phi]"}], "+", "R"}], ")"}], " ", 
      "Sin\[Theta]", " ", 
      OverscriptBox[
       RowBox[{"j", " "}], "^"]}], "+", 
     RowBox[{"r", " ", "Sin\[Phi]"}]}]}]}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.7663541261029263`*^9, 3.766354134332299*^9}},
